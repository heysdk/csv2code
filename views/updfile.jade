extends layout-head

block content
  div(class="container") <br><br><br><br>
    form(role="form", action="updfile", method="post", enctype="multipart/form-data")
      div(class="form-group")
        label(for="csvfile") csv文件
        input(type="file", name="csvfile", id="csvfile")
        p(class="help-block") csv文件，“,”分隔，/r/n换行
      button(type="submit", class="btn btn-default") upload
