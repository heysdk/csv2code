extends layout-head

block content
  div(class="container") <br><br><br><br>
    form(role="form", action="csv2php", method="post")
      div(class="form-group")
        label(for="funcname") 输出的函数名
        input(type="text", class="form-control", name="funcname", id="funcname", placeholder="输出的函数名")
      div(class="form-group")
        label(for="csvinfo") csv文件内容，含表头
        textarea(class="form-control", name="csvinfo", id="csvinfo", rows="20")
      div(class="form-group")
        label(for="csvhead") csv表头配置
        textarea(class="form-control", name="csvhead", id="csvhead", rows="3")
      button(type="submit", class="btn btn-default") 生成代码
